if [ -f $1 ]; then

  if [ "$1" == "/sys/module/omap_temp_sensor/parameters/throttle_enabled" ]; then
    if [[ ! -z $2 ]]; then
      [ "$2" == "1" ] && echo "Y" > $1
      [ "$2" == "0" ] && echo "N" > $1
    fi

    [ `cat $1` == "Y" ] && echo "1"
    [ `cat $1` == "N" ] && echo "0"

  elif [ "$1" == "/proc/sys/vm/lowmem_reserve_ratio" ]; then
    if [[ ! -z $2 ]]; then
      echo "$2\t$2" > $1
    fi

    echo `cat $1 | cut -f1`

  elif [ "$1" == "/data/trimhelper" ]; then
    if [[ ! -z $2 ]]; then
      sed -i "3s/.*/$2/" $1
    fi

    echo `cat $1 | sed -n '3p'`

  else
    if [[ ! -z $2 ]]; then

      echo $2 > $1

      [ "$1" == "/sys/module/panel_s6e8aa0/parameters/contrast" ] && echo `cat /sys/class/misc/colorcontrol/multiplier` > /sys/class/misc/colorcontrol/multiplier
      [ "$1" == "/sys/vibrator/pwmvalue" ] && echo 300 > /sys/devices/virtual/timed_output/vibrator/enable

    fi

    if [ "$1" == "/sys/kernel/logger_mode/logger_mode" ]; then
      echo `cat /sys/kernel/logger_mode/logger_mode | tr -cd [0-9]`
    else
      echo `cat $1`
    fi;

  fi

fi
